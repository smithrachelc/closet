{
  "version": 2,
  "builds": [
    {
      
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/browser" }
    },
    {
      
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      
      "src": "/(.*\\.(?:js|css|json|png|jpg|jpeg|svg|ico|map))$",
      "dest": "/dist/browser/$1"
    },
    {
      
      "src": "/(.*)",
      "dest": "/src/server.ts"
    }
  ]
}
